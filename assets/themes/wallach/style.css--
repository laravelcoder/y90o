@import url("../Divi/style.css");

/* =Customize
-------------------------------------------------------------- */
html,body{
    font-family:'Open Sans', sans-serif;
    background:#fff;
    font-size:17px;
}
/*conversion hack*/
iframe[name='google_conversion_frame'] { 
    height: 0 !important;
    width: 0 !important; 
    line-height: 0 !important; 
    font-size: 0 !important;
    margin-top: -13px;
    float: left;
}
/*end conversion hack*/
.et_header_style_left #et-top-navigation, .et_header_style_split #et-top-navigation {
    padding-top: 0px;
    padding-left:0px !important;
}
/*.home #et-top-navigation,.et_header_style_left .et-fixed-header #et-top-navigation {
    padding-top: 10px;
}*/
#et-top-navigation,.et_header_style_left .et-fixed-header #et-top-navigation {
    padding-top: 0px;
}
.home.page .et_pb_section {
    padding: 0px 0 !important;
    position: relative;
}
.left{
    float:left;
}
h1,h2,h3,nav{
    font-family:'Open Sans Condensed',sans-serif;
    letter-spacing: -0.5px;
    /*text-transform: uppercase;*/
    font-weight: 700;
}
.home h1,.home h2{ display:none;}
.ubermenu .ubermenu-target{ padding:15px 14px}
@media only screen and (min-width: 981px){
    .woocommerce ul.products li.product h3, .woocommerce-page ul.products li.product h3, .et_pb_gallery_grid .et_pb_gallery_item h3, .et_pb_portfolio_grid .et_pb_portfolio_item h2, .et_pb_filterable_portfolio_grid .et_pb_portfolio_item h2, .et_pb_column_1_4 .et_pb_audio_module_content h2 {
