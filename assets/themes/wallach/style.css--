    }
}
nav a, nav span{
    color:#000;
}
/* home hack for compliance */
.homeonly{
    font-size:11px;
    font-weight:normal;
    text-align:right;
    margin-right:10px;
    display:block;
    top: 22px;
    right: 0px;
    position: absolute;
}

.accentcolor{
    color:#191f6b;
}
@media screen and (min-width : 979px) {
   .home .homeonly .independent{
        display:block;
    }
